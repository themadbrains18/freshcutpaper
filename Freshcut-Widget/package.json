{
  "name": "gift-card-generator",
  "version": "0.0.1",
  "description": "Giftcard generator",
  "browser": "../public/giftcard-generator.min.js",
  "scripts": {
    "build:js": "rollup -c",
    "build": "npm run build:js",
    "doc": "typedoc --excludeExternals --externalPattern **/node_modules/** --ignoreCompilerErrors --out ./documentation/ ./src",
    "dev": "rollup -c -w",
    "lint": "eslint src",
    "lint-fix": "eslint --fix src",
    "test": "jest --config jest.config.js --no-cache",
    "compress": "uglifyjs dist/text-right.widget.min.js -o dist/text-right.widget.min.js -c -m --comments /^!/",
    "prebuild": "npm run lint",
    "postbuild": "npm run compress",
    "clean": "rm -rf dist/*"
  },
  "author": "Arturs Juhnevics",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.4.5",
    "@types/chai": "^4.2.3",
    "@types/color": "^3.0.0",
    "@types/jest": "^24.0.22",
    "@types/mocha": "^5.2.7",
    "babel-jest": "^24.9.0",
    "babel-preset-es2015-rollup": "^3.0.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "eslint": "^8.12.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.17.3",
    "jest": "^27.5.1",
    "mocha": "^6.2.1",
    "nyc": "^14.1.1",
    "rollup": "^1.12.5",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-copy": "^3.1.0",
    "rollup-plugin-html": "^0.2.1",
    "rollup-plugin-inject": "^3.0.1",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-resolve": "^5.0.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-scss": "^3.0.0",
    "rollup-plugin-svg": "^2.0.0",
    "rollup-plugin-terser": "^5.0.0",
    "rollup-plugin-typescript": "^1.0.1",
    "rollup-plugin-uglify": "^5.0.2",
    "stylus": "^0.54.5",
    "ts-jest": "^27.1.4",
    "ts-node": "^8.4.1",
